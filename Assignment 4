  int i;
   for(i=1;i<=10;i++)
    printf("%d",2*i-1);  
   return 0;
}
(Answer 05)
#include<stdio.h>
  int main()
{ 
  int i;
   for(i=19;i>=1;i=i-2)
    printf("%d",i);
   return 0;
}
