  int main()
{
    int i;
    for(i=1;i<=10;i++)
    {
        printf("%d",i);
    }
    return 0;
}
(Answer 03)
#include<stdio.h>
  int main()
{
    int i;
    for(i=10;i>=1;i--)
    {
        printf("%d",i);
    }
    return 0;
}
(Answer 04)
